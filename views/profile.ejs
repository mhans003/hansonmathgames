<%- include("./partials/header") %>

<div class="container">
	<div class="row">
		<div class="credentialsform col-xsm-4">
			<div id="highscores">
				<% if(scores.length > 0) { %>
					<h3>High Scores</h3>
					<% for(let i = scores.length - 1; i >= 0; i--) { %>
						<h5>
							<strong><%= scores[i].author.username %>: </strong>
							<br>
							<%= scores[i].game %>
							<br>
							<%= scores[i].score %>
						</h5>
					<% } %>
				<% } %>
			</div>
		</div>
	</div>
</div>

<%- include("./partials/footer") %>